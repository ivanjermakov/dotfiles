!! system
Xcursor.size: 24

Xft.dpi: 96

!! urxvt

! colors
URxvt.foreground: #000000
URxvt.background: #ffffff

! black
*.color0:       #222222
*.color8:       #666666

! red
*.color1:       #e84f4f
*.color9:       #d23d3d

! green
*.color2:       #b7ce42
*.color10:      #bde077

! yellow
*.color3:       #fea63c
*.color11:      #ffe863

! blue
*.color4:       #66aabb
*.color12:      #aaccbb

! magenta
*.color5:       #b7416e
*.color13:      #e16a98

! cyan
*.color6:       #6d878d
*.color14:      #42717b

! white
*.color7:       #dddddd
*.color15:      #cccccc

! appearance
URxvt.font: xft:Consolas:size=15:antialias=true
URxvt.boldFont: xft:Consolas:bold:size=15:antialias=true
URxvt.italicFont: xft:Consolas:italic:size=15:antialias=true
URxvt.boldItalicFont: xft:Consolas:bold:italic:size=15:antialias=true
URxvt.letterSpace: 0 
URxvt.geometry: 92x24
URxvt.internalBorder: 2 
URxvt.cursorBlink: true
URxvt.saveLines: 100000
URxvt.scrollBar: false

! clipboard
URxvt.keysym.Control-v: eval:paste_clipboard
URxvt.keysym.Control-c: eval:selection_to_clipboard
URxvt.iso14755: false
URxvt.iso14755_52: false

! extensions
URxvt.perl-lib: /home/ivan/.config/urxvt/
URxvt.perl-ext-common: default,matcher,resize-font,keyboard-select
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1
URxvt.resize-font.smaller: M-s
URxvt.resize-font.bigger: M-b
URxvt.resize-font.reset: M-r
URxvt.bindsym.M-Escape: perl:keyboard-select:activate

! navigation
URxvt.keysym.Meta-i: command:\033]720;1\007
URxvt.keysym.Meta-k: command:\033]721;1\007
